<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <!--#region jQuery-->
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <!--#endregion-->
    <!--#region bootstrap-->
    <link href="~/Content/assets/bootstrap-3.3.7/dist/css/bootstrap.css" rel="stylesheet" />
    <script src="~/Content/assets/bootstrap-3.3.7/dist/js/bootstrap.js"></script>
    <!--#endregion-->
    <!--#region vue.js-->
    <script src="~/Scripts/vue.js_v2.5.17/dist/vue.min.js"></script>
    <!--#endregion-->
    <!--#region vue-resource-->
    <script src="~/Scripts/vue-resource/dist/vue-resource.js"></script>
    <!--#endregion-->
    @RenderSection("styles", required: false)
</head>
<body>
    @RenderBody()
</body>
</html>
@RenderSection("scripts", required: false)
